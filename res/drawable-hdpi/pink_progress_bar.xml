<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient android:startColor="#F8F8FF" android:endColor="#F8F8FF" android:angle="90" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="#ffced5" android:centerColor="#ffced5" android:endColor="#ffe6ea"
                    android:angle="90" />
                <stroke android:width="1dp" android:color="#e5b9bf" />
            </shape>
        </clip>
    </item>
</layer-list>
